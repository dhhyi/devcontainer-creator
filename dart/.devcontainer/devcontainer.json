{
  "name": "Example devcontainer for dart",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "settings": {
        "files.exclude": {
          ".devcontainer": true,
          ".update_devcontainer.sh": true,
          ".vscode": true
        }
      },
      "extensions": [
        "Dart-Code.dart-code"
      ]
    }
  },
  "containerEnv": {
    "DCC_BINARY": "dart",
    "DCC_REPL": "",
    "DCC_SELFTEST": "Y2F0IDw8RU9GID4gL3RtcC9oZWxsb3dvcmxkLmRhcnQKdm9pZCBtYWluKCkgewogIHByaW50KCdIZWxsbyBXb3JsZCEnKTsKfQpFT0YKZGFydCBydW4gL3RtcC9oZWxsb3dvcmxkLmRhcnQK",
    "DCC_VERSION": "dart --version"
  }
}
