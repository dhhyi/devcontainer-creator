{
  "name": "Example devcontainer for go",
  "build": {
    "dockerfile": "Dockerfile",
    "cacheFrom": "type=registry,ref=ghcr.io/dhhyi/dcc-devcontainer-go-cache",
    "args": {
      "GO_VERSION": "1.20.6"
    }
  },
  "customizations": {
    "vscode": {
      "settings": {
        "files.exclude": {
          ".devcontainer": true,
          ".update_devcontainer.sh": true,
          ".vscode": true
        }
      },
      "extensions": [
        "golang.Go"
      ]
    }
  },
  "containerEnv": {
    "DCC_BINARY": "go run",
