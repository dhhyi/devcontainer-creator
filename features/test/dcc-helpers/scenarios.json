{
  "install_default": {
    "image": "mcr.microsoft.com/vscode/devcontainers/base:debian",
    "features": {
      "dcc-helpers": {}
    },
    "postAttachCommand": "sh -e /disclaimer.sh",
    "containerEnv": {
      "DCC_VERSION": "echo myversion",
      "DCC_BINARY": "bash",
      "DCC_REPL": "myrepl"
    }
  },
  "skip_install_cont": {
    "image": "mcr.microsoft.com/vscode/devcontainers/base:debian",
    "features": {
      "dcc-helpers": {}
    },
    "postAttachCommand": "sh -e /disclaimer.sh",
    "containerEnv": {
      "DCC_VERSION": "echo myversion",
      "DCC_BINARY": "",
      "DCC_REPL": "myrepl"
    }
  },
  "use_onchange_on_node": {
    "image": "mcr.microsoft.com/vscode/devcontainers/javascript-node:16",
    "features": {
      "dcc-helpers": {}
    },
    "postAttachCommand": "sh -e /disclaimer.sh",
    "containerEnv": {
      "DCC_VERSION": "echo myversion",
      "DCC_BINARY": "bash",
      "DCC_REPL": "myrepl"
    }
  }
}
