{
  "name": "Example devcontainer for moonscript",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "LUAROCKS_VERSION": "3.9.1",
      "LUA_VERSION": "5.4.4"
    }
  },
  "settings": {
    "files.exclude": {
      ".devcontainer": true,
      ".update_devcontainer.sh": true,
      ".vscode": true
    }
  },
  "extensions": [
    "sumneko.lua",
    "bubuabu.busted-test-explorer",
    "vgalaktionov.moonscript"
  ],
  "containerEnv": {
    "DCC_BINARY": "moon",
    "DCC_REPL": "",
    "DCC_SELFTEST": "Y2F0ID4vdG1wL2hlbGxvd29ybGQubW9vbiA8PEVPRgpwcmludCAiSGVsbG8gV29ybGQhIgpFT0YKbW9vbiAvdG1wL2hlbGxvd29ybGQubW9vbgo=",
    "DCC_VERSION": "moonc -v ."
  }
}
