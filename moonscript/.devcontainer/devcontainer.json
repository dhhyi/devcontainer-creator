{
  "name": "Example devcontainer for moonscript",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "LUAROCKS_VERSION": "3.9.1",
      "LUA_VERSION": "5.4.4"
    }
  },
  "postAttachCommand": "sh /disclaimer.sh \"${containerWorkspaceFolder}\"",
  "features": {
    "ghcr.io/dhhyi/devcontainer-creator/dcc-helpers:latest": {
      "cont": "true"
    }
  },
  "settings": {
    "files.exclude": {
      ".devcontainer": true,
      ".update_devcontainer.sh": true,
      ".vscode": true
    }
  },
  "extensions": [
    "sumneko.lua",
    "bubuabu.busted-test-explorer",
    "vgalaktionov.moonscript"
  ],
  "remoteUser": "vscode"
}
