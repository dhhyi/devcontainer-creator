{
  "name": "Example devcontainer for factor",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "FACTOR_VERSION": "0.98",
      "WORKSPACE": "${containerWorkspaceFolder}"
    }
  },
  "postAttachCommand": ["sh", "-e", "/disclaimer.sh", "${containerWorkspaceFolder}"],
  "features": {
    "ghcr.io/dhhyi/devcontainer-creator/dcc-helpers:latest": {}
  },
  "settings": {
    "files.exclude": {
      ".devcontainer": true,
      ".update_devcontainer.sh": true,
      ".vscode": true
    }
  },
  "extensions": [
    "dexterhaslem.factor"
  ]
}
