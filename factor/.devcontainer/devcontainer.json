{
  "name": "Example devcontainer for factor",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "FACTOR_VERSION": "0.98",
      "WORKSPACE": "${containerWorkspaceFolder}"
    }
  },
  "settings": {
    "files.exclude": {
      ".devcontainer": true,
      ".update_devcontainer.sh": true,
      ".vscode": true
    }
  },
  "extensions": [
    "dexterhaslem.factor"
  ]
}
