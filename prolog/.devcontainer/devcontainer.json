{
  "name": "Example devcontainer for prolog",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "PROLOG_VERSION": "1.5.0"
    }
  },
  "settings": {
    "files.associations": {
      "*.pl": "prolog"
    },
    "files.exclude": {
      ".devcontainer": true,
      ".update_devcontainer.sh": true,
      ".vscode": true
    }
  },
  "extensions": [
    "rebornix.prolog"
  ]
}
