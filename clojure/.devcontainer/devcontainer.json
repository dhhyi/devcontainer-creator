{
  "name": "Example devcontainer for clojure",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "CLOJURE_VERSION": "1.11.1.1155",
      "JAVA_VERSION": "17"
    }
  },
  "postAttachCommand": "sh /disclaimer.sh \"${containerWorkspaceFolder}\"",
  "features": {
    "ghcr.io/dhhyi/devcontainer-creator/dcc-helpers:latest": {}
  },
  "settings": {
    "files.exclude": {
      ".devcontainer": true,
      ".update_devcontainer.sh": true,
      ".vscode": true
    },
    "files.watcherExclude": {
      "**/target": true
    }
  },
  "extensions": [
    "betterthantomorrow.calva"
  ],
  "remoteUser": "vscode"
}
