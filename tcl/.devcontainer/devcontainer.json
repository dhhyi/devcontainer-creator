{
  "name": "Example devcontainer for tcl",
  "build": {
    "dockerfile": "Dockerfile",
    "cacheFrom": "ghcr.io/dhhyi/dcc-devcontainer-tcl"
  },
  "customizations": {
    "vscode": {
      "settings": {
        "files.exclude": {
          ".devcontainer": true,
          ".update_devcontainer.sh": true,
          ".vscode": true
        }
      },
      "extensions": [
        "bitwisecook.tcl"
      ]
    }
  },
  "containerEnv": {
    "DCC_BINARY": "tclsh",
    "DCC_REPL": "tclsh",
    "DCC_SELFTEST": "ZWNobyAncHV0cyAiSGVsbG8gV29ybGQhIicgfCB0Y2xzaA==",
    "DCC_VERSION": "echo 'puts [info patchlevel]' | tclsh"
  }
}
