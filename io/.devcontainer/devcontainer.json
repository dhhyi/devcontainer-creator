{
  "name": "Example devcontainer for io",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "IO_VERSION": "2019.05.22-alpha"
    }
  },
  "postAttachCommand": "sh -e /disclaimer.sh \"${containerWorkspaceFolder}\"",
  "features": {
    "ghcr.io/dhhyi/devcontainer-creator/dcc-helpers:latest": {}
  },
  "settings": {
    "files.exclude": {
      ".devcontainer": true,
      ".update_devcontainer.sh": true,
      ".vscode": true
    }
  },
  "extensions": [
    "kennethceyer.io"
  ],
  "remoteUser": "vscode"
}
